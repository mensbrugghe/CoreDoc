@Article{Gieseckeetal2013JDS,
  author   = {James A. Giesecke and Nhi Hoang Tran and Erwin L. Corong and Steven Jaffee},
  journal  = {Journal of Development Studies},
  title    = {Rice Land Designation Policy in {Vietnam} and the Implications of Policy Reform for Food Security and Economic Welfare},
  year     = {2013},
  number   = {9},
  pages    = {1202--1218},
  volume   = {49},
  abstract = {Abstract With the aim of promoting national food security, the Vietnamese government enforces the
 designation of around 35 per cent of agricultural land strictly for paddy rice cultivation. We investigate the
 economic effects of adjusting this policy, using an economy-wide model of Vietnam with detailed modelling of region-specific
 land use, agricultural activity, poverty and food security measures. Our results show that the removal of the rice
 land designation policy increases real private consumption by an average of 0.35 per cent per annum
 over 2011--2030, while also reducing poverty, improving food security and contributing to more nutritionally balanced diets among Vietnamese households.},
  doi      = {10.1080/00220388.2013.777705},
}

