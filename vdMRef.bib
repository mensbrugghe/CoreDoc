@Article{AEMuellerRobertson,
  author     = {Christoph M{\"u}ller and Richard D. Robertson},
  journal    = {Agricultural Economics},
  title      = {Projecting future crop productivity for global economic modeling},
  year       = {2014},
  issn       = {1574-0862},
  number     = {1},
  pages      = {37--50},
  volume     = {45},
  abstract   = {Assessments of climate change impacts on agricultural markets and land-use patterns rely on
 quantification of climate change impacts on the spatial patterns of land productivity. We supply a set
 of climate impact scenarios on agricultural land productivity derived from two climate models and two
 biophysical crop growth models to account for some of the uncertainty inherent in climate and impact
 models. Aggregation in space and time leads to information losses that can determine climate change
 impacts on agricultural markets and land-use patterns because often aggregation is across steep
 gradients from low to high impacts or from increases to decreases. The four climate change impact
 scenarios supplied here were designed to represent the most significant impacts (high emission scenario only,
 assumed ineffectiveness of carbon dioxide fertilization on agricultural yields, no adjustments in management)
 but are consistent with the assumption that changes in agricultural practices are covered in the economic models.
 Globally, production of individual crops decrease by 10--38\% under these climate change scenarios,
 with large uncertainties in spatial patterns that are determined by both the uncertainty in climate
 projections and the choice of impact model. This uncertainty in climate impact on crop productivity
 needs to be considered by economic assessments of climate change.},
  bdsk-url-1 = {http://dx.doi.org/10.1111/agec.12088},
  doi        = {10.1111/agec.12088},
  keywords   = {Climate change, Crop modeling, Agricultural productivity, Land use, Q100, Q190, Z000},
}

@Article{MakowskietalAFM2015,
  author   = {D. Makowski and S. Asseng and F. Ewert and S. Bassu and J.L. Durand and T. Li and P. Martre and M. Adam and P.K. Aggarwal and C. Angulo and C. Baron and B. Basso and P. Bertuzzi and C. Biernath and H. Boogaard and K.J. Boote and B. Bouman and S. Bregaglio and N. Brisson and S. Buis and D. Cammarano and A.J. Challinor and R. Confalonieri and J.G. Conijn and M. Corbeels and D. Deryng and G. De Sanctis and J. Doltra and T. Fumoto and D. Gaydon and S. Gayler and R. Goldberg and R.F. Grant and P. Grassini and J.L. Hatfield and T. Hasegawa and L. Heng and S. Hoek and J. Hooker and L.A. Hunt and J. Ingwersen and R.C. Izaurralde and R.E.E. Jongschaap and J.W. Jones and R.A. Kemanian and K.C. Kersebaum and S.-H. Kim and J. Lizaso and M. {Marcaida III} and C. M{\"u}ller and H. Nakagawa and S. Naresh Kumar and C. Nendel and G.J. O'Leary and J.E. Olesen and P. Oriol and T.M. Osborne and T. Palosuo and M.V. Pravia and E. Priesack and D. Ripoche and C. Rosenzweig and A.C. Ruane and F. Ruget and F. Sau and M.A. Semenov and I. Shcherbak and B. Singh and U. Singh and H.K. Soo and P. Steduto and C. St{\"o}ckle and P. Stratonovitch and T. Streck and I. Supit and L. Tang and F. Tao and E.I. Teixeira and P. Thorburn and D. Timlin and M. Travasso and R.P. R{\"o}tter and K. Waha and D. Wallach and J.W. White and P. Wilkens and J.R. Williams and J. Wolf and X. Yin and H. Yoshida and Z. Zhang and Y. Zhu},
  journal  = {Agricultural and Forest Meteorology},
  title    = {A statistical analysis of three ensembles of crop model responses to temperature and {CO2} concentration},
  year     = {2015},
  issn     = {0168-1923},
  pages    = {483--493},
  volume   = {214--215},
  abstract = {Abstract Ensembles of process-based crop models are increasingly used to simulate crop growth for
 scenarios of temperature and/or precipitation changes corresponding to different projections of
 atmospheric {CO2} concentrations. This approach generates large datasets with thousands of simulated crop yield data.
 Such datasets potentially provide new information but it is difficult to summarize them in a useful way
 due to their structural complexities. An associated issue is that it is not straightforward to compare
 crops and to interpolate the results to alternative climate scenarios not initially included in the simulation protocols.
 Here we demonstrate that statistical models based on random-coefficient regressions are able to emulate ensembles
 of process-based crop models. An important advantage of the proposed statistical models is that they can
 interpolate between temperature levels and between {CO2} concentration levels, and can thus be used to calculate
 temperature and [CO2] thresholds leading to yield loss or yield gain, without re-running the original complex crop models.
 Our approach is illustrated with three yield datasets simulated by 19 maize models,
 26 wheat models, and 13 rice models. Several statistical models are fitted to these datasets,
 and are then used to analyze the variability of the yield response to [CO2] and temperature.
 Based on our results, we show that, for wheat, a [CO2] increase is likely to outweigh the negative effect
 of a temperature increase of +2 degree C in the considered sites. Compared to wheat, required levels
 of [CO2] increase are much higher for maize, and intermediate for rice. For all crops,
 uncertainties in simulating climate change impacts increase more with temperature than with elevated [CO2].},
