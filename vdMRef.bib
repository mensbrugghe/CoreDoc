@TechReport{MiniCAM2003,
  author      = {Antoinette L. Brenkert and Son H. Kim and A. J. Smith and Hugh M. Pitcher},
  institution = {Pacific Northwest National Laboratory (PNNL)},
  title       = {Model Documentation for the MiniCAM},
  year        = {2003},
  month       = July,
  number      = {14337},
  abstract    = {The MiniCAM, short for the Mini-Climate Assessment Model, is an integrated assessment model
of moderate complexity focused on energy and agriculture sectors. The model produces
emissions of greenhouse gases (carbon dioxide, methane and nitrous oxide) and other radiatively
important substances such as sulfur dioxide. Through incorporation of the simple climate model
