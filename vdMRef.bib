@Article{RennertetalBPEA2022,
  author  = {Kevin Rennert and Brian C. Prest and William A. Pizer and Richard G. Newell and David Anthoff and Cora Kingdon and Lisa Rennels and Roger Cook and Adrian E. Raftery and Hana {\v{S}ev\v{c}\'ikov\'a} and Frank Errickson},
