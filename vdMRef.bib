@Article{CrespoCuaresma2015,
  author   = {Jes\'us Crespo Cuaresma},
  journal  = {Global Environmental Change},
  title    = {Income projections for climate change research: A framework based on human capital dynamics},
  year     = {2017},
  issn     = {0959-3780},
  pages    = {226--236},
  volume   = {42},
  abstract = {The quantitative assessment of the global effects of climate change requires the construction of income projections
spanning large time horizons. Exploiting the robust link between educational attainment, age structure dynamics and
