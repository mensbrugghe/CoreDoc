  journal  = {Journal of Global Economic Analysis},
  title    = {The Standard GTAP Model in GAMS, Version 7},
  year     = {2018},
  month    = {June},
  number   = {1},
  pages    = {1--83},
  volume   = {3},
  abstract = {The purpose of this paper is to describe a version of the Global Trade Analysis Project (GTAP) computable general equilibrium (CGE)
