@Proceedings{Mastersetal1994,
  title       = {World Petroleum Assessment and Analysis},
  year        = {1994},
  publisher   = {John Wiley \& Sons},
  abstract    = {The occurrence and quantity of world petroleum resources appears to be well understood. The
numbers are so great, however, that even minor variants in the total picture can be responsible for enormous
localized industrial activity. Specific knowledge of the widespread local occurrences of oil and gas, therefore, is
important to economic development and to the free market distribution of energy. It is also clear, however, that
