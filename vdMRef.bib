@Article{DaviesetalEJ2011,
  author    = {James B. Davies and Susanna {Sandstr\"om}, and Anthony Shorrocks and Edward N. Wolff},
  journal   = {The Economic Journal},
  title     = {The Level and Distribution of Global Household Wealth*},
